{
  "name": "expense-manager-backend",
  "version": "1.0.0",
  "description": "Backend for expense management app",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/server.js",
    "client": "cd client && npm start",
    "start": "node server/server.js",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.4-lts.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.10"
  }
}
